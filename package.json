{
  "name": "react-isolate",
  "version": "2.4.20",
  "description": "A React workbench to work/view react component in isolated environment",
  "main": "index.js",
  "scripts": {
    "dev": "concurrent \"npm run watch\" \"node .bin/start\" ",
    "build": "babel _src --out-dir _lib --presets es2015,stage-0,react --plugins jsx-control-statements --no-babelrc",
    "watch": "babel _src --out-dir _lib --presets es2015,stage-0,react --plugins jsx-control-statements --no-babelrc --watch",
    "demo": "./.bin/start.js --componentsPath=demo/components --fixturesPath=demo/fixtures",
    "styles": "./.bin/start.js --componentsPath=demo/components --fixturesPath=demo/fixtures --autoImportLess",
    "lint": "eslint _components _lib",
    "prepublish": "in-publish && npm run build",
    "test": "echo \"Run tests\" && exit 0"
  },
  "contributors": [
    "Eisi Sig <im.eisisig@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/eisisig/react-isolate/issues"
  },
  "bin": {
    "react-isolate": ".bin/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eisisig/react-isolate.git"
  },
  "keywords": [
    "react",
    "isolate",
    "component",
    "workbench",
    "environment"
  ],
  "author": "Eisi Sig @eisisig",
  "dependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-types": "^6.4.5",
    "codemirror": "^5.9.0",
    "component-css-loader": "^0.1.0",
    "component-resolver-webpack": "^0.4.0",
    "css-loader": "^0.23.0",
    "docgen-loader": "^1.3.1",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "highlight.js": "^9.0.0",
    "history": "^1.13.1",
    "html-entities": "^1.2.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^1.7.0",
    "in-publish": "^2.0.0",
    "json5": "^0.4.0",
    "json5-loader": "^0.6.0",
    "jsx-control-statements": "^3.0.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "lodash": "^3.10.1",
    "markdown-loader": "^0.1.7",
    "marked": "^0.3.5",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-docgen": "^2.6.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "style-loader": "^0.13.0",
    "suit-cx": "^1.1.4",
    "tojson-loader": "^1.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.5.0",
    "webpack-sources": "^0.1.0",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-tape-runner": "^2.0.0",
    "concurrently": "^1.0.0",
    "conventional-changelog": "^0.5.1",
    "cz-conventional-changelog": "^1.1.4",
    "eslint": "^1.10.1",
    "eslint-plugin-react": "^3.15.0",
    "tape": "^4.2.2",
    "tape-bdd": "0.0.1"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npm.org"
  }
}
