{
  "name": "react-isolate",
  "version": "2.6.5",
  "description": "A React workbench to work/view react component in isolated environment",
  "main": "index.js",
  "scripts": {
    "server": "node .bin/server",
    "dev": "concurrently \"npm run watch\" \"node .bin/start\" ",
    "build": "babel isolate-src --out-dir isolate-lib --presets es2015,stage-0,react --plugins jsx-control-statements --no-babelrc",
    "watch": "babel isolate-src --out-dir isolate-lib --presets es2015,stage-0,react --plugins jsx-control-statements --no-babelrc --watch",
    "demo": "./.bin/start.js --componentsPath=demo/components --fixturesPath=demo/fixtures",
    "styles": "./.bin/start.js --componentsPath=demo/components --fixturesPath=demo/fixtures --autoImportLess",
    "lint": "eslint _components isolate-lib",
    "prepublish": "in-publish && npm run build",
    "test": "echo \"Run tests\" && exit 0"
  },
  "contributors": [
    "Eisi Sig <im.eisisig@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/eisisig/react-isolate/issues"
  },
  "bin": {
    "react-isolate": ".bin/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eisisig/react-isolate.git"
  },
  "keywords": [
    "react",
    "isolate",
    "component",
    "workbench",
    "environment"
  ],
  "author": "Eisi Sig @eisisig",
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-types": "^6.5.2",
    "codemirror": "^5.12.0",
    "docgen-loader": "^1.3.2",
    "express": "^4.13.4",
    "globby": "^4.0.0",
    "highlight.js": "^9.2.0",
    "history": "^2.0.0",
    "in-publish": "^2.0.0",
    "json5": "^0.4.0",
    "json5-loader": "^0.6.0",
    "jsx-control-statements": "^3.1.0",
    "keo": "^2.0.9",
    "lodash": "^4.5.1",
    "markdown-loader": "^0.1.7",
    "marked": "^0.3.5",
    "raw-loader": "^0.5.1",
    "react": "^0.14.7",
    "react-docgen": "^2.7.0",
    "react-dom": "^0.14.7",
    "react-pure-render": "^1.0.2",
    "react-split-pane": "^0.1.23",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "style-loader": "^0.13.0",
    "tojson-loader": "^1.0.2",
    "webpack": "^2.1.0-beta.2",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^2.0.0-beta",
    "webpack-hot-middleware": "^2.7.1",
    "webpack-sources": "^0.1.1",
    "yargs": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-tape-runner": "^2.0.1",
    "concurrently": "^2.0.0",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "react-addons-test-utils": "^0.14.7",
    "tape": "^4.4.0",
    "tape-bdd": "0.0.1"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "next",
    "registry": "https://registry.npmjs.org/"
  }
}
