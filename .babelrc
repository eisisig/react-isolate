{
  "stage": 0,
  "optional": [
    "es7.decorators"
  ],
  "plugins": [],
  "ignore": ["node_modules"],
  "env": {
    "development": {
      "plugins": [
        "react-transform"
      ],
      "extra": {
        "react-transform": {
          "transforms": [
            {
              "transform": "react-transform-hmr",
              "imports": [
                "react"
              ],
              "locals": [
                "module"
              ]
            }
          ]
        }
      }
    }
  }
}
