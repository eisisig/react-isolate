{
  "babelrc": false,
  "presets": [
    "es2015-webpack",
    "stage-0",
    "react"
  ],
  "plugins": [
    "jsx-control-statements",
    "transform-decorators-legacy"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015"
      ]
    },
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
